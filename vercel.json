{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ],
  "env": {
    "VERCEL": "1",
    "RATE_LIMIT_DEFAULT": "10/minute",
    "RATE_LIMIT_ROOT": "20/minute",
    "RATE_LIMIT_HEALTH": "30/minute",
    "RATE_LIMIT_MODELS": "15/minute",
    "RATE_LIMIT_OPTIMIZE": "10/minute"
  }
}
